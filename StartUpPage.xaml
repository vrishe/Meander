<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Meander"
             xmlns:res="clr-namespace:Meander.Resources.Strings"
             x:Class="Meander.StartUpPage"
             x:DataType="local:StartUpViewModel"
             Title="{x:Static res:Strings.AppTitle}">
    <VerticalStackLayout>
        <Button Text="{x:Static res:Strings.New}"
                Command="{Binding DoNewCommand}"/>
        <Button Text="{x:Static res:Strings.Open}"
                IsEnabled="False"/>
    </VerticalStackLayout>
</ContentPage>